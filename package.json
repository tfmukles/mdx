{
  "name": "sitepins-mdx",
  "version": "1.0.0",
  "description": "Modern MDX processing library for React applications",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-mdx": "^2.0.0",
    "mdast-util-gfm": "^2.0.0",
    "remark-parse": "^10.0.0",
    "remark-mdx": "^2.0.0",
    "remark-gfm": "^3.0.0",
    "unified": "^10.0.0",
    "unist-util-visit": "^4.0.0",
    "vfile": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^4.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^2.0.0",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependencyMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": [
    "mdx",
    "markdown",
    "react",
    "parser",
    "compiler"
  ],
  "author": "SitePins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sitepins/mdx"
  },
  "bugs": {
    "url": "https://github.com/sitepins/mdx/issues"
  },
  "homepage": "https://github.com/sitepins/mdx#readme"
}
